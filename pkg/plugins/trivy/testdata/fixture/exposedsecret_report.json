{
  "SchemaVersion": 2,
  "Results": [
    {
      "Target": "/app/config/secret.yaml",
      "Class": "secret",
      "Secrets": [
        {
          "RuleID": "stripe-publishable-token",
          "Category": "Stripe",
          "Severity": "HIGH",
          "Title": "Stripe",
          "StartLine": 3,
          "EndLine": 3,
          "Match": "publishable_key: *****"
        },
        {
          "RuleID": "stripe-access-token",
          "Category": "Stripe",
          "Severity": "CRITICAL",
          "Title": "Stripe",
          "StartLine": 4,
          "EndLine": 4,
          "Match": "secret_key: *****"
        }
      ]
    },
    {
      "Target": "/etc/apt/s3auth.conf",
      "Class": "secret",
      "Secrets": [
        {
          "RuleID": "aws-access-key-id",
          "Category": "AWS",
          "Severity": "CRITICAL",
          "Title": "AWS Access Key ID",
          "StartLine": 1,
          "EndLine": 1,
          "Match": "AccessKeyId = ********************"
        },
        {
          "RuleID": "aws-secret-access-key",
          "Category": "AWS",
          "Severity": "CRITICAL",
          "Title": "AWS Secret Access Key",
          "StartLine": 2,
          "EndLine": 2,
          "Match": "SecretAccessKey = ****************************************"
        }
      ]
    }
  ]
}
